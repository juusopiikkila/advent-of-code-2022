{
    "compilerOptions": {
        "outDir": "dist",
        "target": "es2019",
        "module": "commonjs",
        "declaration": false,
        "strict": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "sourceMap": true,
        "moduleResolution": "node",
    }
}
